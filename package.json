{
  "name": "@78nine/sideworker",
  "version": "1.0.1",
  "description": "SideWorker is a minimal JavaScript library to make Web Workers usage as painless as possible",
  "main": "sideworker.js",
  "module": "sideworker.js",
  "unpkg": "sideworker.min.js",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "dev": "npm run build -- --watch",
    "serve": "live-server --open=examples",
    "prepublish": "npm run build",
    "test": "live-server --open=tests"
  },
  "files": [
    "sideworker.js",
    "sideworker.min.js",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/78nine/SideWorker.git"
  },
  "keywords": [
    "web worker",
    "worker"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/78nine/SideWorker/issues"
  },
  "homepage": "https://github.com/78nine/SideWorker#readme",
  "devDependencies": {
    "live-server": "1.2.1",
    "rollup": "2.56.0",
    "rollup-plugin-terser": "7.0.2"
  }
}
